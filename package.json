{
  "name": "expo-app",
  "version": "1.0.0",
  "scripts": {
    "android": "expo run:android",
    "ios": "expo run:ios --device 'iPhone 15 Pro'",
    "start": "expo start --clear --dev-client",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\" && tsc",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "clean": "npx expo prebuild --clean",
    "validate": "npx npm-check-updates -i && npx expo@latest install --check && npx expo-doctor@latest",
    "web": "expo start --web"
  },
  "dependencies": {
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@expo/metro-runtime": "~5.0.4",
    "@expo/vector-icons": "14.1.0",
    "@mgcrea/react-native-dnd": "^2.5.3",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-navigation/bottom-tabs": "7.3.13",
    "@react-navigation/elements": "^2.4.2",
    "@react-navigation/native": "7.1.9",
    "@react-navigation/native-stack": "^7.3.13",
    "@react-navigation/stack": "7.3.2",
    "@shopify/flash-list": "1.7.6",
    "@supabase/supabase-js": "2.49.8",
    "date-fns": "^4.1.0",
    "expo": "53.0.9",
    "expo-font": "13.3.1",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.1.7",
    "expo-splash-screen": "0.30.8",
    "expo-status-bar": "2.2.3",
    "expo-store-review": "~8.1.5",
    "expo-system-ui": "5.0.7",
    "lodash": "^4.17.21",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-error-boundary": "^6.0.0",
    "react-native": "0.79.2",
    "react-native-gesture-handler": "2.24.0",
    "react-native-get-random-values": "^1.11.0",
    "react-native-keyboard-controller": "^1.17.2",
    "react-native-reanimated": "~3.17.5",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "4.10.0",
    "react-native-web": "^0.20.0",
    "uuid": "^11.1.0",
    "zustand": "5.0.5"
  },
  "devDependencies": {
    "@babel/core": "7.27.1",
    "@types/lodash": "^4.17.17",
    "@types/react": "19.0.10",
    "@types/uuid": "^10.0.0",
    "eslint": "9.27.0",
    "eslint-config-expo": "9.2.0",
    "eslint-config-prettier": "10.1.5",
    "prettier": "3.5.3",
    "typescript": "5.8.3"
  },
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "packageManager": "pnpm@10.9.0"
}
